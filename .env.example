# --- Basic Authentication (recommended for production) ---
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=supersecurepassword123

# --- Webhook + Deployment Settings ---
N8N_PORT=5678
N8N_PROTOCOL=https
WEBHOOK_URL=https://your-project.up.railway.app/

# Optional: Set this if you are using a custom domain
N8N_HOST=n8n.yourdomain.com

# --- Timezone ---
GENERIC_TIMEZONE=America/New_York

# Optional: disable telemetry
N8N_DIAGNOSTICS_ENABLED=false
